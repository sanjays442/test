{
  "extends": "eslint-config-es5",
  "globals": {
    "angular": true
  },
  "rules": {
    "one-var": 0,
    "func-names": 0,
    "global-require": 0,
    "guard-for-in": 0,
    "no-undefined": 0,
    "no-use-before-define": ["error", {
      "functions": false
    }],
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "never",
      "asyncArrow": "always"
    }]
  }
}
